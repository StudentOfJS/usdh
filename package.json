{
  "name": "usdh",
  "author": "Rod Lewis",
  "description": "useData Hook for Cache API",
  "version": "1.0.7",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/StudentOfJS/usdh.git"
  },
  "keywords": [
    "typescript",
    "react",
    "cache api",
    "hook",
    "fetch"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies;\"",
    "test": "vitest --config ./vitest.config.ts",
    "coverage": "vitest run --coverage"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/usdh.umd.js",
  "module": "./dist/usdh.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/usdh.es.js",
      "require": "./dist/usdh.umd.js"
    }
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@vitejs/plugin-react": "^3.0.1",
    "happy-dom": "^8.1.1",
    "json": "^11.0.0",
    "react": "^18.2.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.2.0",
    "vitest": "^0.26.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
